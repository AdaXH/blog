{
  "apps": [
    {
      "name": "koa",
      "cwd": "./",
      "script": "./koa.js",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "error_file": "log/error/node-app.stderr.log",
      "out_file": "log/node-app.stdout.log",
      "pid_file": "log/node-geo-api.pid",
      "instances": 6,
      "min_uptime": "200s",
      "max_restarts": 10,
      "max_memory_restart": "100M",
      "cron_restart": "1 0 * * *",
      "watch": false,
      "merge_logs": true,
      "exec_interpreter": "node",
      "exec_mode": "cluster_mode",
      "autorestart": false,
      "vizion": false,
      "env": {
        "NODE_ENV": "production"
      }
    }
  ]
}
