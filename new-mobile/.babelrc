{
  "presets": ["env", "react", "stage-3"],
  "plugins": [
    "dva-hmr",
    "transform-decorators-legacy",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ],
    "transform-class-properties",
    "transform-runtime",
    "transform-async-generator-functions",
    "transform-async-to-generator",
    "syntax-dynamic-import"
  ],
  "env": {
    "production": {
      "plugins": ["transform-remove-console"]
    }
  }
}
