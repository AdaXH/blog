{
  "name": "blog",
  "version": "2.0.0",
  "description": "",
  "main": "koa.js",
  "engines": {
    "node": "12.18.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node koa.js --env=production",
    "dev": "node koa.js --env=production",
    "lint-client": "cd dva-version && eslint --ext .js src test",
    "dev-pc": "npm-run-all --parallel server pc-server",
    "dev-mobile": "npm-run-all --parallel server mobile-server",
    "build-pc": "npm run rm-old && cd pc && npm run build",
    "build-mobile": "npm run rm-mobile && cd umi-mobile && npm run build",
    "rm-old": "rm -r public/pc-src",
    "rm-mobile": "rm -r public/mobile",
    "server": "nodemon -i ./pc/ koa -i ./umi-mobile -i ./views --env=development",
    "pc-server": "cd pc && npm run dev",
    "mobile-server": "cd umi-mobile && npm run dev",
    "lint-server": "eslint ./controllers/**/*.js ./middleware/**/*.js ./common/**/*.js",
    "lint-server-fix": "eslint --fix ./controllers/**/*.js ./middleware/**/*.js ./common/**/*.js",
    "precommit": "npm run lint-server",
    "push": "node push.js"
  },
  "author": "Ada",
  "license": "ISC",
